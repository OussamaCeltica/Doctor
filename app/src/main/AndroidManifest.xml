<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.celtica.doctor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Food.QuickResponseActivity"></activity>
        <activity android:name=".Food.DetailRecetteActivity" />
        <activity android:name=".Food.ResultRecetteActivity" />
        <activity android:name=".Food.RecetteParNutriActivity" />
        <activity android:name=".Food.RecetteIngredientActivity" />
        <activity android:name=".Food.NutritionActivity" />
        <activity android:name=".DiagnoResultActivity" />
        <activity android:name=".MaladieDescription" />
        <activity android:name=".DiagnostiqueActivtity" />
        <activity android:name=".MaladiesActivity" />
        <activity android:name=".Accueil">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>